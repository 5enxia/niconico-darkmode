{
    "manifest_version": 2,
    "name": "niconico-darkmode.",
    "version": "0.1.0",
    "description": "niconicoをダークモードにします",
    "content_scripts": [
        {
            "matches": [
                "https://www.nicovideo.jp/*"
            ],
            "css": [
                "parts/header.css","www_nicovideo_jp/_.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/video_top*"
            ],
            "css": [
                "parts/header.css","www_nicovideo_jp/video_top.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/watch/*"
            ],
            "css": [
                "parts/header.css","www_nicovideo_jp/watch.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/search/*",
                "https://www.nicovideo.jp/tag/*"
            ],
            "css": [
                "parts/header.css","www_nicovideo_jp/serch.css"
            ]
        }
    ]
}