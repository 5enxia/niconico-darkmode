{
    "manifest_version": 2,
    "name": "niconico-darkmode.",
    "version": "0.2.0",
    "description": "niconicoサイトをダークモード化する拡張機能",
    "content_scripts": [
        {
            "matches": [
                "https://www.nicovideo.jp/",
                "https://www.nicovideo.jp/?*"
            ],
            "css": [
                "parts/header.css",
                "top/_.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/daily_topics/*"
            ],
            "css": [
                "parts/header.css",
                "top/daily_topics.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/recommendnews",
                "https://www.nicovideo.jp/recommendnews*"
            ],
            "css": [
                "parts/header.css",
                "video/parts/zero_design.css",
                "top/recommendnews.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/video_top",
                "https://www.nicovideo.jp/video_top?*",
                "https://www.nicovideo.jp/video_top/genre*"
            ],
            "css": [
                "parts/header.css",
                "video/parts/cre_design.css",
                "video/video_top.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/watch/*"
            ],
            "css": [
                "parts/header.css",
                "video/watch.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/search/*",
                "https://www.nicovideo.jp/tag/*",
                "https://www.nicovideo.jp/mylist_search/*",
                "https://www.nicovideo.jp/newarrival*",
                "https://www.nicovideo.jp/recent*",
                "https://www.nicovideo.jp/related_tag*"
            ],
            "css": [
                "parts/header.css",
                "video/parts/zero_design.css",
                "video/serch.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/rss*"
            ],
            "css": [
                "parts/header.css",
                "top/rss.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/my/*",
                "https://www.nicovideo.jp/user/*"
            ],
            "css": [
                "parts/header.css",
                "video/parts/zero_design.css",
                "video/mypage/my.css",
                "video/mypage/user.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/ranking*"
            ],
            "css": [
                "parts/header.css",
                "video/parts/cre_design.css",
                "video/ranking.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/recommendations*"
            ],
            "css": [
                "parts/header.css",
                "video/parts/cre_design.css",
                "video/recommendations.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/video_catalog*"
            ],
            "css": [
                "parts/header.css",
                "video/parts/zero_design.css",
                "video/other.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/",
                "https://live.nicovideo.jp/?*"
            ],
            "css": [
                "parts/header.css",
                "live/parts/live_parts.css",
                "live/parts/card.css",
                "live/live_top.css"
            ]
        },
        {
            "matches": [
                "https://live2.nicovideo.jp/focus",
                "https://live2.nicovideo.jp/focus?*"
            ],
            "css": [
                "parts/header.css",
                "live/parts/live_parts.css",
                "live/parts/card.css",
                "live/forcus.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/recent",
                "https://live.nicovideo.jp/recent?*"
            ],
            "css": [
                "parts/header.css",
                "live/parts/live_parts.css",
                "live/parts/card.css",
                "live/recent.css"
            ]
        }
    ]
}