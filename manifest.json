{
    "manifest_version": 2,
    "name": "niconico-darkmode.",
    "version": "0.1.0",
    "description": "niconicoをダークモードにします",
    "content_scripts": [
        {
            "matches": [
                "https://www.nicovideo.jp/*"
            ],
            "css": [
                "parts/header.css","www_nicovideo_jp/_.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/daily_topics/*"
            ],
            "css": [
                "parts/header.css","www_nicovideo_jp/daily_topics.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/video_top*"
            ],
            "css": [
                "parts/header.css","www_nicovideo_jp/video_top.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/watch/*"
            ],
            "css": [
                "parts/header.css","www_nicovideo_jp/watch.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/search/*",
                "https://www.nicovideo.jp/tag/*",
                "https://www.nicovideo.jp/mylist_search/*",
                "https://www.nicovideo.jp/newarrival*",
                "https://www.nicovideo.jp/recent*"
            ],
            "css": [
                "parts/header.css","www_nicovideo_jp/serch.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/rss*"
            ],
            "css": [
                "parts/header.css","www_nicovideo_jp/rss.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/my/*"
            ],
            "css": [
                "parts/header.css","www_nicovideo_jp/mypage/my.css"
            ]
        }
    ]
}